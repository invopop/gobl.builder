{
  "name": "@invopop/gobl-builder",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "prebuild": "npm run install-gobl-wasm",
    "build": "vite build",
    "build:package": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "rimraf node_modules .svelte-kit build package",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "install-gobl-wasm": "node scripts/install-gobl-wasm.js",
    "postinstall": "npm run install-gobl-wasm"
  },
  "devDependencies": {
    "@fontsource/fira-code": "^4.5.10",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "next",
    "@tailwindcss/forms": "^0.5.2",
    "@types/base-64": "^1.0.0",
    "@types/country-list": "^2.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/golang-wasm-exec": "^1.15.0",
    "@types/utf8": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.7",
    "base-64": "^1.0.0",
    "clsx": "^1.2.1",
    "country-list": "^2.2.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "file-saver": "^2.0.5",
    "flowbite-svelte": "^0.26.2",
    "monaco-editor": "^0.33.0",
    "node-fetch": "^3.2.6",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rimraf": "^3.0.2",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "utf8": "^3.0.0",
    "vite": "^3.1.0-beta.1"
  }
}
