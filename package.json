    "svelte-hero-icons": "^5.0.0",
    "svelte-intersection-observer-action": "^0.0.4",
    "svelte-select": "^5.7.0",
    "svelte-toasts": "^1.1.2",
    "utf8": "^3.0.0",
    "vscode-json-languageservice": "5.3.5"
  },
  "overrides": {
    "monaco-editor": {
      "vscode-json-languageservice": "$vscode-json-languageservice"
    }
  }
}
